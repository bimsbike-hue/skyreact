{
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    { "src": "/assets/.*", "headers": { "cache-control": "public, max-age=31536000, immutable" } },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    { "src": "/robots.txt", "dest": "/robots.txt" },
    { "src": "/sitemap.xml", "dest": "/sitemap.xml" },
    { "src": "/.*", "dest": "/" }
  ]
}
